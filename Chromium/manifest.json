{
  "manifest_version": 3,
  "name": "ORSys Sniper",
  "version": "3.1",
  "permissions": [
    "storage",
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "https://or.ump.edu.my/*"
  ],
  "action": {
    "default_title": "ORSys Sniper â†’ v3.1 | Made By: Dani",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/light/icon16.png",
      "32": "icons/light/icon32.png",
      "48": "icons/light/icon48.png",
      "128": "icons/light/icon128.png"
    }
  },
  "background": {
    "service_worker": "bg.js"
  },
  "icons": {
    "16": "icons/light/icon16.png",
    "32": "icons/light/icon32.png",
    "48": "icons/light/icon48.png",
    "128": "icons/light/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://or.ump.edu.my/*"],
      "js": ["content.js"]
    }
  ]
}
